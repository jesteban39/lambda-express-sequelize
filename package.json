{
  "name": "lambda-express-sequelize",
  "version": "1.0.0",
  "description": "Backend para servicios de TI",
  "main": "src/index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "test": "jest --coverage && npm run format",
    "test-w": "jest --watch src/.+ts",
    "dev": "cross-env NODE_ENV=dev serverless offline",
    "format": "prettier --write \"*.{ts,json}\"  \"{src,tests}/**/*.{ts,json}\"",
    "commit": "git add . && cat > commit.txt && git commit -F commit.txt && rm -rf commit.txt && git push"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.3.3",
    "sequelize": "^6.33.0",
    "serverless-http": "^3.2.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.7",
    "@types/js-yaml": "^4.0.6",
    "@types/morgan": "^1.9.6",
    "@types/swagger-ui-express": "^4.1.4",
    "aws-sdk": "^2.1467.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.4",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.1",
    "pg-hstore": "^2.3.4",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.4",
    "serverless-esbuild": "^1.26.1",
    "serverless-offline": "^13.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.6.3"
  },
  "nodemonConfig": {
    "watch": "src",
    "ext": "ts",
    "exec": "cross-env NODE_ENV=dev ts-node --files -r tsconfig-paths/register ./src"
  }
}
